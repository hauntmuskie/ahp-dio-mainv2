	<queryString>
		<![CDATA[SELECT 
		    id_supplier, 
		    (@rownum := @rownum + 1) AS ranking, 
		    nama_supplier, 
		    hasil_penilaian 
		FROM seleksi, (SELECT @rownum := 0) r 
		ORDER BY hasil_penilaian DESC]]>
	</queryString>
